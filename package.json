{
  "name": "electron",
  "version": "1.0.0",
  "description": "",
  "main": "main/main.js",
  "scripts": {
    "dev": "concurrently \"yarn dev:vite\" \" yarn dev:electron\"",
    "dev:vite": "vite",
    "dev:electron": "yarn build:electron && electron .",
    "build": "yarn build:vite && yarn build:electron",
    "build:vite": "vite build",
    "build:electron": "tsc -p electron",
    "dist": "npm run build && electron-builder",
    "pack": "npm run build && electron-builder --dir"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qkrdmstlr3/electron.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/qkrdmstlr3/electron/issues"
  },
  "homepage": "https://github.com/qkrdmstlr3/electron#readme",
  "dependencies": {
    "@vanilla-extract/css": "^1.9.2",
    "framer-motion": "^8.0.2",
    "polished": "^4.2.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^18.11.17",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@vanilla-extract/vite-plugin": "^3.7.0",
    "@vitejs/plugin-react": "^3.0.0",
    "concurrently": "^7.6.0",
    "dmg-builder": "^24.0.0-alpha.8",
    "electron": "17.1.2",
    "electron-builder": "22.14.13",
    "electron-is-dev": "^2.0.0",
    "eslint": "^8.30.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "prettier": "^2.8.1",
    "typescript": "^4.9.3",
    "vite": "^4.0.0"
  },
  "build": {
    "asar": true,
    "files": [
      "main",
      "dist"
    ],
    "directories": {
      "buildResources": "resources"
    }
  },
  "packageManager": "yarn@3.3.1"
